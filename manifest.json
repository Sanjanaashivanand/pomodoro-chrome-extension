{
    "name": "Pomodoro Timer",
    "version": "1.0.0",
    "description": "An extension for pomodoro timer",
    "manifest_version": 3,
    "author": "Sanjana Shivananda",
    "action": {
      "default_popup": "index.html",
      "default_title": "Pomodoro Timer",
      "default_icon": "favicon_io/favicon-16x16.png"
    },
    "icons": {
      "16": "favicon_io/favicon-16x16.png",
      "32": "favicon_io/favicon-32x32.png",
      "48": "favicon_io/android-chrome-192x192.png",
      "128": "favicon_io/android-chrome-512x512.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "settings.html",
    "permissions": [
      "alarms",
      "notifications",
      "storage",
      "scripting"
    ]
}
  